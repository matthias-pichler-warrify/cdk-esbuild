  "dependencies": {
    "esbuild": "^0.13.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-lambda": "^1.99.0",
    "@aws-cdk/aws-s3-assets": "^1.99.0",
    "@aws-cdk/aws-s3-deployment": "^1.99.0",
    "@aws-cdk/aws-synthetics": "^1.99.0",
    "@aws-cdk/core": "^1.99.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.99.0",
    "@types/jest": "ts4.4",
    "@types/node": "ts4.4",
    "jest": "^27.0.3",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aws-cdk",
